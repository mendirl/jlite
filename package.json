{
  "name": "jhipster-sample-application",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Sample Application",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:tsc": "vue-tsc -p tsconfig.build.json --noEmit",
    "build:vite": "vite build --emptyOutDir",
    "dev": "npm-run-all --parallel dev:*",
    "dev:vite": "vite",
    "e2e": "playwright test --ui --config src/test/webapp/e2e/playwright.config.ts",
    "e2e:headless": "playwright test --config src/test/webapp/e2e/playwright.config.ts",
    "lint": "eslint .",
    "prepare": "husky",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "preview": "vite preview",
    "start": "vite",
    "test": "npm run watch:test",
    "test:component": "start-server-and-test dev http://localhost:9000 'playwright test --ui --config src/test/webapp/component/playwright.config.ts'",
    "test:component:headless": "start-server-and-test dev http://localhost:9000 'playwright test --config src/test/webapp/component/playwright.config.ts'",
    "test:coverage": "vitest run --coverage",
    "watch": "npm-run-all --parallel watch:*",
    "watch:test": "vitest --",
    "watch:tsc": "npm run build:tsc -- --watch"
  },
  "dependencies": {
    "axios": "1.12.0",
    "piqure": "2.1.1",
    "vue": "3.5.25",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@playwright/test": "1.52.0",
    "@prettier/plugin-xml": "3.4.1",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/coverage-istanbul": "4.0.16",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-vue": "10.1.0",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.0.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.5.3",
    "prettier-plugin-gherkin": "3.1.2",
    "prettier-plugin-java": "2.7.7",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.20",
    "start-server-and-test": "2.0.12",
    "typescript": "5.8.3",
    "typescript-eslint": "8.48.1",
    "vite": "6.4.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.16",
    "vitest-sonar-reporter": "2.0.1",
    "vue-tsc": "2.2.10"
  },
  "engines": {
    "node": ">=22"
  }
}
